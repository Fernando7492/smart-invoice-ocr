generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoice {
  id            String   @id @default(uuid())
  fileName      String
  filePath      String
  extractedText String?  @db.Text
  status        String   @default("PENDING")
  createdAt     DateTime @default(now())
}